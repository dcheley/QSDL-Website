<div class="container">
  <h3>News</h3>
  <li class="divider"></li>
  <% @news.each do |article| %>
    <h4><%= article.title %></h4>
    <p class="article-content"><%= article.content %><p>
      <b>Updated:</b> <%= article.updated_at.strftime("%a, %B %d, %Y") %>
    <li class="divider"></li>
  <% end %>
</div></br>
